{
  "version": 1,
  "projects": {
    "tune": {
      "projectType": "application",
      "root": "apps/tune",
      "sourceRoot": "apps/tune/src",
      "prefix": "mauromattos00",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/tune",
            "index": "apps/tune/src/index.html",
            "main": "apps/tune/src/main.ts",
            "polyfills": "apps/tune/src/polyfills.ts",
            "tsConfig": "apps/tune/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/tune/src/favicon.ico", "apps/tune/src/assets"],
            "styles": [
              "libs/tune-ui-styles/src/index.scss",
              "apps/tune/src/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "libs/tune-ui-styles/src/lib/components",
                "libs/tune-ui-styles/src/lib/helpers",
                "libs/tune-ui-styles/src/lib/typography"
              ]
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/tune/src/environments/environment.ts",
                  "with": "apps/tune/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "tune:build:production"
            },
            "development": {
              "browserTarget": "tune:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tune:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tune/src/**/*.ts",
              "apps/tune/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tune"],
          "options": {
            "jestConfig": "apps/tune/jest.config.js",
            "passWithNoTests": true,
            "styles": []
          }
        }
      },
      "tags": []
    },
    "tune-e2e": {
      "root": "apps/tune-e2e",
      "sourceRoot": "apps/tune-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tune-e2e/cypress.json",
            "devServerTarget": "tune:serve:development",
            "tsConfig": "apps/tune-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tune:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/tune-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["tune"]
    },
    "tune-ui": {
      "root": "libs/tune-ui",
      "sourceRoot": "libs/tune-ui/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/tune-ui/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tune-ui"],
          "options": {
            "jestConfig": "libs/tune-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "tune-ui-styles": {
      "root": "libs/tune-ui-styles",
      "sourceRoot": "libs/tune-ui-styles/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/tune-ui-styles/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tune-ui-styles"],
          "options": {
            "jestConfig": "libs/tune-ui-styles/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "video-player": {
      "root": "libs/video-player",
      "sourceRoot": "libs/video-player/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/video-player/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/video-player"],
          "options": {
            "jestConfig": "libs/video-player/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
